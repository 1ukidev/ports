# description	: List of country, language and currency names
# homepage	: https://pkg-isocodes.alioth.debian.org/
# maintainer	: Emmett1, emmett1.2miligrams at gmail.com
# depends	: python3

name=iso-codes
version=3.79
release=1
source=(https://salsa.debian.org/iso-codes-team/iso-codes/uploads/ef8de8bc12e0512d26ed73436a477871/$name-$version.tar.xz)
md5sum=(5875d7aa41b1acd7b218631ff7b85464)

build() {
	cd $name-$version

	./configure --prefix=/usr
	make
	make DESTDIR=$PKG install
}
