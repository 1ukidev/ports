# description	: List of country, language and currency names
# homepage	: https://pkg-isocodes.alioth.debian.org/
# maintainer	: Emmett1, emmett1.2miligrams at gmail.com
# depends	: python3

name=iso-codes
version=3.77
release=1
source=(https://salsa.debian.org/iso-codes-team/iso-codes/uploads/053dcee773a67523328628c92dd7e919/$name-$version.tar.xz)
md5sum=(9d0d06cfb4634428b300845edcd7140a)

build() {
	cd $name-$version

	./configure --prefix=/usr
	make
	make DESTDIR=$PKG install
}
