name=man-pages
version=5.01
release=1
source=(https://www.kernel.org/pub/linux/docs/$name/$name-$version.tar.xz)
md5sum=(5f75f72efcbe5cd245c4d9f36005d070)

build() {
	cd $name-$version
	make DESTDIR=$PKG install
}
