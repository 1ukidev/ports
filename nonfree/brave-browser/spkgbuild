name=brave-browser
description="A simple web browser"
version=1.35.103
source="https://github.com/brave/brave-browser/releases/download/v$version/brave-browser_${version}_amd64.deb"
depends="libxscrnsaver alsa-lib gtk3 nss"
release=1

build() {
	bsdtar -xf brave-browser_${version}_amd64.deb
	bsdtar -xf data.tar.xz -C $PKG/
}
