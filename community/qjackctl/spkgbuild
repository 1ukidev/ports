# description	: JACK Audio Connection Kit - Qt GUI Interface
# homepage	: https://qjackctl.sourceforge.io/
# depends	: qt jack alsa

name=qjackctl
version=0.6.0
release=1
source=(https://sourceforge.net/projects/qjackctl/files/latest/download/qjackctl-$version.tar.gz)
md5sum=(96f9547d61bf963704df988a5074bda0)

build() {
	cd $name-$version

	./configure --prefix=/usr \
	make
	make DESTDIR=$PKG install
	
}
