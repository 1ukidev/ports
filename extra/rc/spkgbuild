# description	: BSD-style init (CRUX's init inspired)
# backup	: etc/rc.conf etc/rc.local
# depends	: sysvinit

name=rc
version=20180814
release=1
options=(!strip)
_commit=97389f232be7f00a0e186bf107d9d5f91fa062e1
source=($name-$_commit.tar.gz::https://github.com/emmett1/rc/archive/$_commit.tar.gz)
md5sum=(7baff686ecf964c3900d8ca3c979e233)

build() {
	cd $name-$_commit
	make DESTDIR=$PKG install
	
}
