# description	: MATE menu editing tooldictionaries and searching files
# depends	: mate-menus

name=mozo
version=1.22.2
release=1
source=(http://pub.mate-desktop.org/releases/${version:0:4}/$name-$version.tar.xz)

build() {
	cd $name-$version
	./configure --prefix=/usr --disable-nls
	make
	make DESTDIR=$PKG install
}
