# description	: A small youtube browser
# depends	: hicolor-icon-theme qt5 qt5-webkit

name=smtube
version=18.3.0
release=1
source=(https://downloads.sourceforge.net/projects/smtube/files/SMTube/$version/smtube-$version.tar.bz2)
md5sum=(81652002de0006da52361faebe7bd9f2)

build() {
	cd $name-$version
	make PREFIX=/usr
	make PREFIX=/usr DESTDIR=$PKG install
}
