# description	: C-Extensions for Python3
# depends	: python3

name=cython3
version=0.29.5
release=1
source=(cython-$version.tar.gz::https://github.com/cython/cython/archive/$version.tar.gz)
md5sum=(d7415c69f1f5e55d806440cd9a3fd36b)

build() {
	cd cython-$version
	python3 setup.py install --root=$PKG
}
