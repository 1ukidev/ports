# description	: Intel ACPI Source Language compiler
# depends	: glibc

name=iasl
version=20180105
release=1
source=(https://acpica.org/sites/acpica/files/acpica-unix-$version.tar.gz
)

build() {
	cd acpica-unix-$version/generate/unix
	make clean
	make
	make DESTDIR=$PKG install
}

md5sum=(8768636b53a3cfa0659275614dfd8ce9)
