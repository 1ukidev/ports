#
# /etc/rc.d/vboxservice: start/stop VirtualBox Guest Service
#

NAME="VirtualBox Service"
PROG=/usr/bin/VBoxService
OPTS="-f"

sv_start() {
	msg "Starting $NAME..."
	start_service $PROG $OPTS &
}

sv_stop() {
	msg "Stopping $NAME..."
	stop_service $PROG
}

sv_status() {
	status_service $PROG
}

sv_restart() {
	sv_stop
	sleep 1
	sv_start
}

sv_usage() {
	echo "usage: [start|stop|restart|status]"
}

# End of file
