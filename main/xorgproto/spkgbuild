# description	: Combined X.Org X11 Protocol headers
# depends	: meson

name=xorgproto
version=2022.1
release=1
source="https://xorg.freedesktop.org/archive/individual/proto/$name-$version.tar.bz2"

build() {
	cd $name-$version
	venom-meson build
	meson compile -C build
	DESTDIR=$PKG meson install -C build
}
