# description :  goffice 
# depends	: gtk3 librsvg libxslt which libgsf

name=goffice
version=0.10
release=50
source="https://download.gnome.org/sources/$name/$version/$name-$version.$release.tar.xz"

build() {
	cd $name-$version.$release
    ./configure --prefix=/usr
    make
	make PREFIX=$PKG/usr DESTDIR=$PKG install
}
