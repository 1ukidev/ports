name=rc
version=20210512
release=1
backup="etc/rc.conf"
source="https://github.com/venomlinux/rc/archive/c1f623591cf82fb35906cf2f9a729b6c361c4e5f.tar.gz"

build() {
	cd $name-c1f623591cf82fb35906cf2f9a729b6c361c4e5f
	make DESTDIR=$PKG install
}
