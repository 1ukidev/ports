#!/bin/sh
#
# /etc/rc.d/rsync: start/stop RSYNC Server
#

. /etc/rc.subr

NAME="TLP daemon"
PROG=/usr/bin/tlp
OPTS="init"

case $1 in
	start)
		msg "Starting $NAME..."
		start_daemon $PROG $OPTS
		;;
	stop)
		msg "Stopping $NAME..."
		stop_daemon $PROG
		;;
	restart)
		$0 stop
		sleep 1
		$0 start
		;;
	status)
		status_daemon $PROG
		;;
	*)
		echo "usage: $0 [start|stop|restart|status]"
		;;
esac

# End of file
